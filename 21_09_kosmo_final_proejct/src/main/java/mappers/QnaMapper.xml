<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="QnaDAO">

<insert id="insertQnaBoard" parameterType="qnaboard">
	INSERT INTO CMIS_QNA_BOARD(MEMBER_NAME, QNA_BOARD_CONTENT, QNA_BOARD_DATE) VALUES(#{member_name},#{qna_board_content},NOW())
</insert>

<select id="getQnaBoardList" resultType="qnaboard">
	SELECT * FROM CMIS_QNA_BOARD
</select>
		
</mapper>